msgid "IP addresses"
msgstr "Direcciones IP"

#. type: Content of: <ul><li>
#: admin/views/default/help/en/settings_software.html:38
msgid "Serial number and key"
msgstr "Número de serie y clave"

#. type: Content of: <ul><li>
#: admin/views/default/help/en/settings_software.html:40
msgid "RAM memory size"
msgstr "Tamaño de memoria RAM"

#. type: Content of: <ul><li>
#: admin/views/default/help/en/settings_software.html:42
